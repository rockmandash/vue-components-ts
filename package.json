{
  "name": "vue-components-ts",
  "version": "0.1.10",
  "scripts": {
    "build": "node scripts/makeRootIndexFile.js && vue-cli-service build --target lib --formats commonjs --name main src/main.ts",
    "storybook:build": "vue-cli-service storybook:build -c config/storybook",
    "storybook:serve": "vue-cli-service storybook:serve -p 6006 -c config/storybook",
    "release": "node ./scripts/autoCommitRelease.js && yarn run build && npm publish",
    "format": "prettier --config ./.prettierrc --write '**/*.{ts,js,css,html,json,vue,mdx}'"
  },
  "main": "dist/main.common.js",
  "typings": "dist/main.d.ts",
  "peerDependencies": {
    "vue": ">=2.1.8"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.3.13",
    "@storybook/addon-backgrounds": "^5.3.13",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-docs": "^5.3.13",
    "@storybook/addon-knobs": "^5.3.13",
    "@storybook/addon-links": "^5.3.13",
    "@storybook/addon-notes": "^5.3.13",
    "@storybook/addon-viewport": "^5.3.13",
    "@storybook/vue": "^5.3.13",
    "@types/fs-extra": "^8.1.0",
    "@types/glob": "^7.1.1",
    "@vue/cli-plugin-babel": "^4.2.2",
    "@vue/cli-plugin-typescript": "^4.2.2",
    "@vue/cli-service": "^4.2.2",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.6",
    "node-sass": "^4.13.1",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.2",
    "standard-version": "^7.1.0",
    "storybook-addon-paddings": "^1.0.1",
    "typescript": "~3.7.5",
    "vue": "^2.6.11",
    "vue-cli-plugin-storybook": "^1.2.0",
    "vue-template-compiler": "^2.6.11"
  },
  "files": [
    "dist"
  ]
}
